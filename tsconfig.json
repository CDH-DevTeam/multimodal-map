{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.json"
  ],
  "exclude": [
    "projects/etruscan/**/*"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "noImplicitAny": false,
    "allowJs": true,
    "lib": ["es2019", "dom"]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
